    "package": "cd tools && npm run cli:package",
    "package:test": "cd tools && npm run cli:package:test",
    "package:publish": "cd tools && npm run cli:package:publish",
    "demo:build": "cd tools && npm run cli:demo",
    "serve": "cd tools && npm run cli:serve",
    "e2e": "ng e2e",
    "start": "ng serve",
    "delete:reports": "rm -r cypress/results/* || true",
    "prereport": "npm run delete:reports",
    "mochawesome:merge": "npx mochawesome-merge \"cypress/results/mochawesome/*.json\" > mochawesome.json && npx marge mochawesome.json",
    "junit:merge": " npx junit-merge -d cypress/results/junit -o cypress/results/junit/result.xml",
    "cypress:open": "cypress open",
    "cypress:run": "npm run prereport & cypress run & npm run mochawesome:merge & npm run junit:merge",
    "cypress:dashboard": "npx cypress run --record --key f02e45b5-be6a-423e-bc6b-ff1ef8667624",
    "cypress:run:chrome": "npm run prereport & cypress run --browser chrome",
    "cypress:run:firefox": "npm run prereport & cypress run --browser firefox",
    "cypress:run:edge": "npm run prereport & cypress run --browser edge",
    "cypress:e2e": "start-test start http://localhost:8000 cypress:run"
  },
